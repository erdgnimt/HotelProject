@model List<BookingResultDTO>
<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-body">
                <div class="active-member">
                    <div class="table-responsive">
                        <table class="table table-xs mb-0">
                            <thead>
                                <tr>
                                    <th>Misafir</th>
                                    <th>Otel Giriş Tarihi</th>
                                    <th>Otel Çıkış Tarihi</th>
                                    <th>Durum</th>
                                    <th>İletişim</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                  
                                <tr>
                                    <td>@item.Name</td>
                                        <td>@(((DateTime)item.CheckIn).ToString("dd-MMM-yyyy"))</td>
                                    <td>
                                            <span>@(((DateTime)item.CheckOut).ToString("dd-MMM-yyyy"))</span>
                                    </td>
                                    <td>@item.Status</td>
                                    <td><i class="fa fa-circle-o text-success  mr-2"></i>@item.Mail</td> 
                                </tr>  
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>